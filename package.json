{
  "name": "react-mobx-boilerplate",
  "version": "0.0.1",
  "description": "A react boilerplate project featuring webpack, HMR, code splitting, mobx, SSR",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node webpack/webpack-dev-server --env.dev",
    "build": "rm -Rf build/* && npm run build-browser && npm run build-node",
    "build-browser": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --progress --env.prod",
    "build-node": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.node.js --progress --env.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/react-mobx-boilerplate.git"
  },
  "author": "Sharon Rolel <sharonrolel@gmail.com>",
  "homepage": "https://github.com/mobxjs/react-mobx-boilerplate",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "classnames": "^2.2.6",
    "date-fns": "^2.10.0",
    "highcharts": "^8.0.3",
    "highcharts-react-official": "^3.0.0",
    "local-storage-fallback": "^4.1.1",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "routes": "2.1.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "connect-history-api-fallback": "1.5.0",
    "mini-css-extract-plugin": "0.4.1",
    "opn": "5.3.0",
    "autoprefixer": "8.6.4",
    "awesome-typescript-loader": "5.2.0",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "express": "4.16.3",
    "express-history-api-fallback": "2.2.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "postcss": "6.0.23",
    "postcss-loader": "2.1.5",
    "style-loader": "0.21.0",
    "tslib": "1.9.3",
    "typescript": "3.8.3",
    "url-loader": "1.0.1",
    "webpack": "4.15.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  }
}
